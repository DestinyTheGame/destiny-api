{
  "name": "destiny-api",
  "version": "1.0.0",
  "description": "Destiny API through bungie.net",
  "main": "index.js",
  "scripts": {
    "100%": "istanbul check-coverage --statements 100 --functions 100 --lines 100 --branches 100",
    "test": "mocha test.js",
    "watch": "mocha --watch test.js",
