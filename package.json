  "license": "MIT",
  "dependencies": {
    "async": "0.9.x",
    "emits": "3.0.x",
    "eventemitter3": "2.0.x",
    "extendible": "0.1.x",
    "failure": "1.1.x",
    "millisecond": "0.1.x",
    "modification": "1.0.x",
    "propget": "1.0.0",
    "url-parse": "1.1.x",
    "xmlhttprequest": "1.8.0"
  },
  "devDependencies": {
    "assume": "1.4.x",
    "istanbul": "0.4.x",
    "mocha": "3.2.x",
    "pre-commit": "1.2.x"
  }
}
